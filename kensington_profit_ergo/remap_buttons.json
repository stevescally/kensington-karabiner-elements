            {
              "repeat": false,
              "simultaneous": [
                  {
                      "key_code": "left_command"
                  },
                  {
                      "key_code": "left_shift"
                  },
                  {
                      "key_code": "h"
                  }
              ]
            }
          ]
        }
      ]
    }
  ]
